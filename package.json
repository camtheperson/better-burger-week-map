{
  "name": "burger-week-map",
  "version": "1.0.0",
  "description": "Interactive map for Portland Mercury's Burger Week 2025",
  "main": "index.js",
  "scripts": {
    "scrape": "node scripts/scraper.js",
    "geocode": "node scripts/geocode.js",
    "scrape-and-geocode": "npm run scrape && npm run geocode",
    "dev": "http-server -p 8080 -c-1",
    "build": "./scripts/build.sh",
    "deploy": "npm run build && gh-pages -d dist",
    "start": "npm run dev",
    "add-coords": "node scripts/add-coordinates.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.10",
    "node-geocoder": "^4.3.0"
  },
  "devDependencies": {
    "gh-pages": "^6.1.0",
    "http-server": "^14.1.1"
  },
  "keywords": [
    "portland",
    "burger-week",
    "map",
    "restaurants"
  ],
  "author": "Cameron Hermens",
  "license": "MIT"
}
