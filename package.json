  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.10",
    "node-geocoder": "^4.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.1.0",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "tailwindcss": "^4.1.12",
    "vite": "^4.5.14"
  },
  "keywords": [
    "portland",
    "burger-week",
    "map",
    "restaurants"
