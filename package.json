{
  "name": "burger-week-map",
  "version": "1.0.0",
  "type": "module",
  "description": "Interactive map for Portland Mercury's Burger Week 2025",
  "main": "index.js",
  "scripts": {
    "scrape": "node scripts/scraper.js",
    "geocode": "node scripts/google-geocode.js",
    "test-geocode": "node scripts/test-google-geocode.js",
    "scrape-images": "node scripts/scrape-images.js",
    "scrape-and-geocode": "npm run scrape && npm run geocode",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "start": "npm run dev"
